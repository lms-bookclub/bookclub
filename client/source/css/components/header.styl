.c-header
  display flex
  justify-content space-between
  align-items center
  margin-bottom 5px
  border-bottom 1px solid #f2f2f2
  background #fff

  &__avatar
    width 30px
    height 30px
    padding 3px
    border-radius 50%
    cursor pointer

  &__nav-tabs
    padding 0
    margin 0
    list-style none
    display inline-block
